{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Rl-Training-ADF"
		},
		"Archive_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://sqlserverstor.blob.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://RLTrainingKeyVault.vault.azure.net/"
		},
		"Recon_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rltraining.database.windows.net"
		},
		"Recon_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "recon"
		},
		"Recon_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ralackey305"
		},
		"Source_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://sqlserverstor.blob.core.windows.net/"
		},
		"azure_recon_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rltraining.database.windows.net"
		},
		"azure_recon_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Recon"
		},
		"azure_recon_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ralackey305"
		},
		"bankFile_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://sqlserverstor.blob.core.windows.net/"
		},
		"linkedService1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://sqlserverstor.blob.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Rl-Training-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Archive_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('Archive_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Source_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('Source_properties_typeProperties_serviceEndpoint')]"
					},
					"bankFile_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('bankFile_properties_typeProperties_serviceEndpoint')]"
					},
					"linkedService1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('linkedService1_properties_typeProperties_serviceEndpoint')]"
					},
					"Recon_properties_typeProperties_server": {
						"value": "[parameters('Recon_properties_typeProperties_server')]"
					},
					"Recon_properties_typeProperties_database": {
						"value": "[parameters('Recon_properties_typeProperties_database')]"
					},
					"Recon_properties_typeProperties_userName": {
						"value": "[parameters('Recon_properties_typeProperties_userName')]"
					},
					"azure_recon_properties_typeProperties_server": {
						"value": "[parameters('azure_recon_properties_typeProperties_server')]"
					},
					"azure_recon_properties_typeProperties_database": {
						"value": "[parameters('azure_recon_properties_typeProperties_database')]"
					},
					"azure_recon_properties_typeProperties_userName": {
						"value": "[parameters('azure_recon_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}